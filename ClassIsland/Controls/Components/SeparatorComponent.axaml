<ci:ComponentBase x:Class="ClassIsland.Controls.Components.SeparatorComponent"
             xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ClassIsland.Controls.Components"
             xmlns:ci="http://classisland.tech/schemas/xaml/core"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Width="2">
        <Line VerticalAlignment="Center" Stroke="{DynamicResource TextFillColorSecondaryBrush}" StrokeThickness="2"
              HorizontalAlignment="Center" StartPoint="0,0" EndPoint="0,25" Opacity="0.8">
        </Line>
    </Grid>
</ci:ComponentBase>
