<UserControl x:Class="ClassIsland.Controls.AuthorizeProviderPresenter"
             xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ClassIsland.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             Loaded="Control_OnLoaded"
             Unloaded="Control_OnUnloaded">
    <Grid>
        <ContentPresenter Content="{Binding DisplayingContent, Mode=OneWay, RelativeSource={RelativeSource FindAncestor, AncestorType=local:AuthorizeProviderPresenter}}"></ContentPresenter>
    </Grid>
</UserControl>
